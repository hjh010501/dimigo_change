<UserControl x:Class="dimigopositionchange.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:dimigopositionchange"
             mc:Ignorable="d" 
             Width="165"
             Height="55"
             d:DesignHeight="55" d:DesignWidth="165" MinWidth="165" MinHeight="55">
    <Grid>
        <Rectangle Fill="White" HorizontalAlignment="Left" Height="55" Stroke="Black" VerticalAlignment="Top" Width="165" StrokeThickness="2"/>
        <TextBlock x:Name="Name" Text="{Binding Text}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18"/>

    </Grid>
</UserControl>
